{
  "name": "trench-js",
  "version": "0.0.1",
  "description": "Trench JS client for browser and Node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix",
